<% layout('layouts/default') -%>

<% script('//www.google.com/jsapi') %>
<% script('/javascripts/bootstrap-sortable.js') %>
<% script('/javascripts/stats.js') %>

<h1>Global Javascript Statistics</h1>

<blockquote>
  <p>Let's be realistic about how many people <em>still</em> don't have Javascript in their browser!</p>
  <small>An atypical front end developer</small>
</blockquote>

<div class="progress progress-striped active">
  <div class="progress-bar progress-bar-info"  role="progressbar" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100" style="width: 20%">
    <span class="sr-only">Loading</span>
  </div>
</div>

<div class="alert alert-danger" style="display: none"></div>

<div id="stats-container" class="global">
  <%- partial('_stats-container') %>
</div>